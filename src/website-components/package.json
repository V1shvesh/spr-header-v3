{
  "name": "@sprinklrjs/website-components",
  "version": "5.0.6-rc.3",
  "description": "Website Header",
  "main": "index.js",
  "module": "./esm/index.js",
  "scripts": {
    "tsc": "tsc",
    "release": "./release.sh",
    "copy-package.json": "cp package.json dist/",
    "build:cjs": "tsc --module commonjs --outDir ./dist && copyfiles -u 1 \"./src/**/*.{scss,svg}\" \"./dist\"",
    "build:esm": "tsc --module es2015 --outDir ./dist/esm && copyfiles -u 1 \"./src/**/*.{scss,svg}\" \"./dist/esm\"",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./dist/styles.css",
    "build": "yarn clean && yarn build:cjs && yarn build:esm && yarn build:css && yarn copy-package.json",
    "build:watch": "yarn build:cjs --watch",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "contentful": "^10.6.14",
    "fast-safe-stringify": "^2.1.1",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.292.0",
    "node-fetch": "^2.6.9",
    "react-markdown": "^8.0.1",
    "react-use": "^17.3.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@sprinklr/eslint-config-sprinklr-com": "1.0.0",
    "@sprinklr/prettier-config": "1.0.0",
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/blocks": "^7.0.22",
    "@storybook/preset-create-react-app": "^7.0.22",
    "@storybook/react": "^7.0.22",
    "@storybook/react-webpack5": "^7.0.22",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/lodash.throttle": "^4.1.9",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.1",
    "eslint": "^8.33.0",
    "eslint-plugin-storybook": "^0.6.12",
    "lightningcss-cli": "^1.23.0",
    "storybook": "^7.0.22",
    "typescript": "^4.6.2",
    "webpack": "^5.87.0"
  }
}
